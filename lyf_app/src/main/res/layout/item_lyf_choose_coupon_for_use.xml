<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/item_lyf_choose_coupon_for_use_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/distance_8"
        android:paddingLeft="@dimen/distance_15"
        android:paddingTop="@dimen/distance_8"
        android:text="久久丫优惠券"
        android:textColor="@color/sub_title_color" />

    <com.ody.p2p.views.MyListView
        android:id="@+id/item_lyf_choose_coupon_for_use_mylistview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:divider="@color/white"
        android:dividerHeight="10dp" />

</LinearLayout>
