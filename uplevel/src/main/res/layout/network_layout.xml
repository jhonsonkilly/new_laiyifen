<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/circular_topbead_border"
    android:orientation="vertical">


    <TextView
        android:id="@+id/tv_uotitle"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/circular_topbead_border"
        android:gravity="center"
        android:text="titile"
        android:textColor="@color/text3color"
        android:textSize="14sp" />

    <View style="@style/view_line" />

    <TextView
        android:id="@+id/tv_dialog_confirm"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="确定"
        android:textColor="@color/color_bule"
        android:textSize="14sp" />

    <View style="@style/view_line" />

    <TextView
        android:id="@+id/tv_dialog_cancle"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="取消"
        android:textColor="@color/text3color"
        android:textSize="14sp" />
</LinearLayout>
